<h2>Users</h2>
  <table class="table">
    <tr>
      <th>image</th>
      <th>name</th>
      <th></th>
    </tr>
    <tr>
    <% user.each do |user| %>
      <td><%= image_tag user.get_profile_image(50,50) %></td>
      <td><%= user.name %></td>
      <td><%= link_to "Show",user_path(user.id) %></td>
    </tr>
    <% end %>
  </table>